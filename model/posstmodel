const mongoose=require("mongoose")
const patientmodel = require("./patientmodel")
const postSchema=new mongoose.Schema(
    {
        userid:{
            type:mongoose.Schema.type.ObjectId,
            required:true,
            ref:"user"
        },
        post:{
            type:String,
            required:true,

        },
        postDate:{
            type:Date,
            default:Date.now
        }
    }
)
module.exports=mongoose.model("patient",patientmodel)